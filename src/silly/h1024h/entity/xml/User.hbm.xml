<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="silly.h1024h.entity.User" table="user" schema="h1024h">
        <id name="uId">
            <column name="u_id" sql-type="int(11)"/>
        </id>
        <property name="uName">
            <column name="u_name" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="uPassword">
            <column name="u_password" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="uDspName">
            <column name="u_dsp_name" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="uSex">
            <column name="u_sex" sql-type="int(11)"/>
        </property>
        <property name="uPhone">
            <column name="u_phone" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="uEmail">
            <column name="u_email" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="uVip">
            <column name="u_vip" sql-type="int(11)"/>
        </property>
        <property name="uCreateTime">
            <column name="u_create_time" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="uToken">
            <column name="u_token" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>